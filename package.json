{
	"name": "whatis.wags.fm",
	"version": "0.0.0",
	"description": "A brief intro to wags",
	"homepage": "https://whatis.wags.fm",
	"subdomain": "revealjs",
	"module": "dist/reveal.esm.js",
	"license": "MIT",
	"scripts": {
		"start": "spago install && spago build && webpack serve --open",
		"build": "webpack --mode=production"
	},
	"author": {
		"name": "Hakim El Hattab",
		"email": "hakim.elhattab@gmail.com",
		"web": "https://hakim.se"
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/hakimel/reveal.js.git"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"keywords": [
		"reveal",
		"slides",
		"presentation"
	],
	"devDependencies": {
		"@babel/core": "^7.16.7",
		"@babel/eslint-parser": "^7.14.3",
		"@babel/preset-env": "^7.16.8",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^19.0.0",
		"@rollup/plugin-node-resolve": "^13.0.0",
		"babel-loader": "^8.2.3",
		"babel-plugin-transform-html-import-to-string": "0.0.1",
		"colors": "^1.4.0",
		"core-js": "^3.12.1",
		"css-loader": "^6.5.1",
		"fitty": "^2.3.0",
		"glob": "^7.1.7",
		"gulp": "^4.0.2",
		"gulp-autoprefixer": "^5.0.0",
		"gulp-clean-css": "^4.2.0",
		"gulp-connect": "^5.7.0",
		"gulp-eslint": "^6.0.0",
		"gulp-header": "^2.0.9",
		"gulp-tap": "^2.0.0",
		"gulp-zip": "^4.2.0",
		"highlight.js": "^10.0.3",
		"marked": "^2.0.3",
		"node-qunit-puppeteer": "^2.1.0",
		"purescript": "^0.14.5",
		"purs-tidy": "^0.6.4",
		"qunit": "^2.17.2",
		"rollup": "^2.48.0",
		"rollup-plugin-terser": "^7.0.2",
		"sass": "^1.47.0",
		"sass-loader": "^12.4.0",
		"spago": "^0.20.3",
		"style-loader": "^3.3.1",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.7.2",
		"yargs": "^15.1.0"
	},
	"browserslist": "> 0.5%, IE 11, not dead",
	"eslintConfig": {
		"env": {
			"browser": true,
			"es6": true
		},
		"parser": "@babel/eslint-parser",
		"parserOptions": {
			"sourceType": "module",
			"allowImportExportEverywhere": true,
			"requireConfigFile": false
		},
		"globals": {
			"module": false,
			"console": false,
			"unescape": false,
			"define": false,
			"exports": false
		},
		"rules": {
			"curly": 0,
			"eqeqeq": 2,
			"wrap-iife": [
				2,
				"any"
			],
			"no-use-before-define": [
				2,
				{
					"functions": false
				}
			],
			"new-cap": 2,
			"no-caller": 2,
			"dot-notation": 0,
			"no-eq-null": 2,
			"no-unused-expressions": 0
		}
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^5.15.4",
		"notyf": "^3.10.0",
		"url": "^0.11.0"
	}
}
